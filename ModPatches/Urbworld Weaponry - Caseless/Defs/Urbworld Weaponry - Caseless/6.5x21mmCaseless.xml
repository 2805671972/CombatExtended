	<!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef>
		<defName>AmmoSet_65x21mmCaseless</defName>
		<label>6.5x21mm Caseless</label>
		<ammoTypes>
			<Ammo_65x21mmCaseless_FMJ>Bullet_65x21mmCaseless_FMJ</Ammo_65x21mmCaseless_FMJ>
