  <!-- === Accessory === -->
  
  <RecipeDef ParentName="CE_AttachmentRecipeBase">
    <defName>Make_AccessoryMakeshift</defName>
    <label>make makeshift accessory component</label>
    <description>Make a makeshift accessory component.</description>
    <jobString>Making makeshift accessory component.</jobString>
    <workAmount>5000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <CE_AccessoryMakeshift>1</CE_AccessoryMakeshift>
    </products>
  </RecipeDef>
  
