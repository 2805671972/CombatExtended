	<ThingDef Class="CombatExtended.AmmoDef" ParentName="Base12GaugeBullet">
		<defName>Bullet_12Gauge_SHTBuck</defName>
		<label>shitty birdshot pellet</label>
		<graphicData>
			<texPath>Things/Projectile/Shotgun_Pellet</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<tickerType>Normal</tickerType>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>4</damageAmountBase>
			<pelletCount>24</pelletCount>
			<armorPenetrationSharp>0.85</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
			<spreadMult>15.8</spreadMult>
		</projectile>
		<comps>
			<li Class="VWEPATCH.RandDamageProps">
					<max_damage>1</max_damage>
					<!--To code lurkers, max damage doesn't do anything yet-->
					<pelletsMin>5</pelletsMin>
					<pelletsMax>24</pelletsMax>
			</li>
		</comps>	
	</ThingDef>
	
