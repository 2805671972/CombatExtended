<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rim-Skeletons</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
          <xpath>Defs/BodyPartDef[defName="MechanicalJaw"
            or defName="MechEye"]/tags</xpath>
					<value>
						<li>OutsideSquishy</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
</Patch>
