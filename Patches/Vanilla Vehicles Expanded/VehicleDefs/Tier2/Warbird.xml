<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Vehicles Expanded</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>

				<!-- Warbird -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/Vehicles.VehicleDef[defName="VVE_Warbird"]/statBases/ArmorRating_Blunt</xpath>
					<value>
						<ArmorRating_Blunt>6</ArmorRating_Blunt>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/Vehicles.VehicleDef[defName="VVE_Warbird"]/statBases/ArmorRating_Sharp</xpath>
					<value>
						<ArmorRating_Sharp>3</ArmorRating_Sharp>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>