<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Remove small rockets(explosive guns added to CE's rocket research) -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/ResearchProjectDef[defName="RF_SmallRockets"]</xpath>
	</Operation>
	<!-- Remove antimateriel weapons (guns added to CE_AdvancedFirearms) -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/ResearchProjectDef[defName="RF_AntiMateriel"]</xpath>
	</Operation>
	<!-- Remove plasteel gun assembly (guns added to CE_AdvancedFirearms and Charged shot) -->
	<Operation Class="PatchOperationRemove">
		<xpath>*/ResearchProjectDef[defName="RF_PlasteelGuns"]</xpath>
	</Operation>


</Patch>

