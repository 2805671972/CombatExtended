<?xml version="1.0" encoding="utf-8" ?>
<Patch>	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rim-Shek</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				  <li Class="PatchOperationAdd">
					<xpath>
					  /Defs/BodyDef[defName="ShekBody"]//*[
					  def="Torso" or 
					  def="Neck" or 
					  def="Head" or 
					  def="Jaw" or 
					  def="Ear" or
					  def="Nose" or
					  def="Shoulder" or
					  def="Arm" or
					  def="Hand" or
					  def="Finger" or
					  def="Foot" or
				  def="Toe" or
					  def="Leg"]/groups
					</xpath>
					<value>
					  <li>CoveredByNaturalArmor</li>
					</value>
				  </li>

				  <li Class="PatchOperationAdd">
					<xpath>
					  /Defs/BodyDef[defName="ShekBody"]//*[
					  def="Jaw" or 
					  def="Nose" or
					  def="Eye"]/groups
					</xpath>
					<value>
					  <li>OutsideSquishy</li>
					</value>
				  </li>

			</operations>
		</match>
	</Operation>
</Patch>
