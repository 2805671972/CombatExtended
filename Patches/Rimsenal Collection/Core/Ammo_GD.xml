<Patch>

	<Operation Class="PatchOperationFindMod">
    <mods>
		<li>Rimsenal - Core</li>
    </mods>
		<match Class="PatchOperationSequence">
		 <operations>		


		<li Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>

	<!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef>
		<defName>AmmoSet_556x45mmNATOHybrid</defName>
		<label>5.56x45mm NATO</label>
		<ammoTypes>
			<Ammo_556x45mmNATO_FMJ>Bullet_556x45mmNATOHybrid_FMJ</Ammo_556x45mmNATO_FMJ>
			<Ammo_556x45mmNATO_AP>Bullet_556x45mmNATOHybrid_AP</Ammo_556x45mmNATO_AP>
			<Ammo_556x45mmNATO_HP>Bullet_556x45mmNATOHybrid_HP</Ammo_556x45mmNATO_HP>
		    <Ammo_556x45mmNATO_Incendiary>Bullet_556x45mmNATOHybrid_Incendiary</Ammo_556x45mmNATO_Incendiary>
			<Ammo_556x45mmNATO_HE>Bullet_556x45mmNATOHybrid_HE</Ammo_556x45mmNATO_HE>
			<Ammo_556x45mmNATO_Sabot>Bullet_556x45mmNATOHybrid_Sabot</Ammo_556x45mmNATO_Sabot>				
		</ammoTypes>
	</CombatExtended.AmmoSetDef>

	<!-- ================== Projectiles ================== -->

	<ThingDef Class="CombatExtended.AmmoDef" Name="Base556x45mmNATOHybridBullet" ParentName="BaseBullet" Abstract="true">
		<graphicData>
			<texPath>Things/Projectile/Bullet_Small</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Bullet</damageDef>
			<speed>222</speed>
			<dropsCasings>true</dropsCasings>
		</projectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="Base556x45mmNATOHybridBullet">
		<defName>Bullet_556x45mmNATOHybrid_FMJ</defName>
		<label>5.56mm NATO bullet (FMJ)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>16</damageAmountBase>
			<armorPenetrationSharp>7</armorPenetrationSharp>
			<armorPenetrationBlunt>49.18</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="Base556x45mmNATOHybridBullet">
		<defName>Bullet_556x45mmNATOHybrid_AP</defName>
		<label>5.56mm NATO bullet (AP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>10</damageAmountBase>
			<armorPenetrationSharp>14</armorPenetrationSharp>
			<armorPenetrationBlunt>49.18</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="Base556x45mmNATOHybridBullet">
		<defName>Bullet_556x45mmNATOHybrid_HP</defName>
		<label>5.56mm NATO bullet (HP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>20</damageAmountBase>
			<armorPenetrationSharp>4</armorPenetrationSharp>
			<armorPenetrationBlunt>49.18</armorPenetrationBlunt>
		</projectile>
	</ThingDef>

	  <ThingDef Class="CombatExtended.AmmoDef" ParentName="Base556x45mmNATOHybridBullet">
		<defName>Bullet_556x45mmNATOHybrid_Incendiary</defName>
		<label>5.56mm NATO bullet (AP-I)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
		  <damageAmountBase>10</damageAmountBase>
		  <armorPenetrationSharp>14</armorPenetrationSharp>
		  <armorPenetrationBlunt>49.18</armorPenetrationBlunt>
		  <secondaryDamage>
			<li>
			  <def>Flame_Secondary</def>
			  <amount>6</amount>
			</li>
		  </secondaryDamage>
		</projectile>
	  </ThingDef>
	  
	  <ThingDef Class="CombatExtended.AmmoDef" ParentName="Base556x45mmNATOHybridBullet">
		<defName>Bullet_556x45mmNATOHybrid_HE</defName>
		<label>5.56mm NATO bullet (HE)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
		  <damageAmountBase>16</damageAmountBase>
		  <armorPenetrationSharp>7</armorPenetrationSharp>
		  <armorPenetrationBlunt>49.18</armorPenetrationBlunt>
		  <secondaryDamage>
				<li>
			  	<def>Bomb_Secondary</def>
			  	<amount>9</amount>
				</li>
		  </secondaryDamage>
		</projectile>
	  </ThingDef>

	  <ThingDef Class="CombatExtended.AmmoDef" ParentName="Base556x45mmNATOHybridBullet">
		<defName>Bullet_556x45mmNATOHybrid_Sabot</defName>
		<label>5.56mm NATO bullet (Sabot)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
		  <damageAmountBase>8</damageAmountBase>
		  <armorPenetrationSharp>24.5</armorPenetrationSharp>
		  <armorPenetrationBlunt>63.66</armorPenetrationBlunt>
		  <speed>332</speed>
		</projectile>
	  </ThingDef>

		</value>
		</li>

		</operations>
		</match>
	</Operation>
</Patch>
			