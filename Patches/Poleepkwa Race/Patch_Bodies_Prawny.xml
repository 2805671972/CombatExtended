<?xml version="1.0" encoding="utf-8" ?>
<Patch>	
	<Operation Class="PatchOperationFindMod">
		<mods>
		<li>Poleepkwa Race Update</li>
		<li>Poleepkwa Race</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

					  <!-- Natural Armor -->

				  <li Class="PatchOperationAdd">
					<xpath>
					  /Defs/BodyDef[defName="PoleepkwaBody"]//*[
					  def="Torso" or 
					  def="Neck" or 
					  def="Head" or 
					  def="Jaw" or 
					  def="Ear" or
					  def="Nose" or
					  def="Shoulder" or
					  def="Arm" or
					  def="Hand" or
					  def="Finger" or
					  def="Leg" or 
					  def="Toe" or 
					  def="Foot"]/groups
					</xpath>
					<value>
					  <li>CoveredByNaturalArmor</li>
					</value>
				  </li>

						<li Class="PatchOperationAdd">
						  <xpath>
							/Defs/BodyDef[defName="PoleepkwaBody"]//*[
							def="Nose" or 
							def="Jaw" or 
							def="Eye" ]/groups
						  </xpath>
						  <value>
							<li>OutsideSquishy</li>
						  </value>
						</li>

			</operations>
		</match>
	</Operation>
</Patch>