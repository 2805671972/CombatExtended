					<li Class="PatchOperationReplace">
						<xpath>Defs/ThingDef[defName="RNGun_UMP45HRT_PDW"]/tools</xpath>
						<value>
							<tools>
								<li Class="CombatExtended.ToolCE">
									<label>stock</label>
