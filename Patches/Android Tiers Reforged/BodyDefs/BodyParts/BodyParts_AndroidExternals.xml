<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Android Tiers Reforged</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs/BodyPartDef[defName="ATR_VoiceSynthesizer"
						or defName="ATR_MechanicalVisualSensor"
						or defName="ATR_MechanicalJaw"]/tags</xpath>

					<value>
						<li>OutsideSquishy</li>
					</value>
				</li>

				<li Class="PatchOperationConditional">
					<xpath>Defs/BodyPartDef[defName="ATR_SmellSensor"]/tags</xpath>

					<match Class="PatchOperationAdd">
						<xpath>Defs/BodyPartDef[defName="ATR_SmellSensor"]/tags</xpath>

						<value>
							<li>OutsideSquishy</li>
						</value>
					</match>

					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/BodyPartDef[defName="ATR_SmellSensor"]</xpath>

						<value>
							<tags>
								<li>OutsideSquishy</li>
							</tags>
						</value>
					</nomatch>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
