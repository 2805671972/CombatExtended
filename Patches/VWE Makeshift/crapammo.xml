<?xml version="1.0" encoding="utf-8" ?>

<Patch>

  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Combat Extended</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>

               
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_762x39mmSoviet"]/ammoTypes</xpath>
                   <value>
                        <Ammo_762x39mmSoviet_Arabic>Bullet_762x39mmSoviet_Arabic</Ammo_762x39mmSoviet_Arabic>                  
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_22LR"]/ammoTypes</xpath>
                   <value>
                        <Ammo_22LR_MakeShift>Bullet_22LR_J</Ammo_22LR_MakeShift>                  
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_762x54mmR"]/ammoTypes</xpath>
                   <value>
                        <Ammo_762x54mmR_Arabic>Bullet_762x54mmR_SHT</Ammo_762x54mmR_Arabic>                  
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_12Gauge"]/ammoTypes</xpath>
                   <value>
                        <Ammo_12Gauge_Arabic>Bullet_12Gauge_SHTBuck</Ammo_12Gauge_Arabic>                  
                    </value>
                </li>
                                          
                       
      </operations>
    </match>
  </Operation>
  
</Patch>